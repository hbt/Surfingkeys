{
  "name": "Surfingkeys",
  "version": "1.17.12",
  "description": "Map your keys for web surfing, expand your browser with javascript and keyboard.",
  "main": "background.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:doc": "documentation build src/content_scripts/common/api.js src/content_scripts/common/normal.js src/content_scripts/common/clipboard.js src/content_scripts/common/hints.js src/content_scripts/common/visual.js src/content_scripts/front.js src/user_scripts/index.js -f md -o docs/api.md",
    "build:doc-cmds": "node scripts/generate-command-docs.js",
    "build:doc-chrome-api": "bash scripts/fetch-all-chrome-apis.sh",
    "build:dev": "node ./config/esbuild.config.js development",
    "build:prod": "node ./config/esbuild.config.js production",
    "build": "npm-run-all clean test build:doc build:doc-chrome-api build:prod",
    "watch": "node ./config/esbuild.config.js development --watch",
    "test": "jest --config=config/jest.config.js",
    "test:cdp:headless": "node tests/cdp/run-headless.js",
    "test:cdp:headless:all": "find tests/cdp -name '*.test.ts' -type f | xargs -P 16 -I {} npm run test:cdp:headless {}",
    "test:cdp:live": "node tests/cdp/run-live.js",
    "debug:cdp:headless": "node debug/run-headless.js",
    "debug:cdp:live": "node debug/run-live.js",
    "lint": "eslint --config config/eslint.config.js src tests debug scripts --ext .js,.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brookhong/Surfingkeys.git"
  },
  "author": "brook hong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brookhong/Surfingkeys/issues"
  },
  "homepage": "https://github.com/brookhong/Surfingkeys#readme",
  "devDependencies": {
    "@types/chrome": "^0.1.35",
    "@types/events": "^3.0.0",
    "@types/jest-image-snapshot": "^4.3.1",
    "@types/lodash": "^4.14.172",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "documentation": "^14.0.3",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.39.2",
    "jest": "^27.3.1",
    "jest-image-snapshot": "^4.5.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^24.20.0",
    "strict-event-emitter-types": "^2.0.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.5.4",
    "ws": "^8.19.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.7.0",
    "@pixi/app": "^7.4.0",
    "@pixi/constants": "^7.4.0",
    "@pixi/core": "^7.4.0",
    "@pixi/display": "^7.4.0",
    "@pixi/extensions": "^7.4.0",
    "@pixi/graphics": "^7.4.0",
    "@pixi/math": "^7.4.0",
    "@pixi/runner": "^7.4.0",
    "@pixi/settings": "^7.4.0",
    "@pixi/sprite": "^7.4.0",
    "@pixi/ticker": "^7.4.0",
    "@pixi/unsafe-eval": "^7.4.0",
    "@pixi/utils": "^7.4.0",
    "ace-builds": "^1.4.12",
    "aws4fetch": "^1.0.20",
    "dompurify": "^3.2.4",
    "events": "^3.3.0",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.21",
    "marked": "^4.0.10",
    "pdfjs-dist": "4.8.69"
  }
}
